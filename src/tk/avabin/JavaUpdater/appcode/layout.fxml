<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="320.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tk.avabin.JavaUpdater.appcode.Controller">
   <children>
      <GridPane prefHeight="320.0" prefWidth="480.0" style="-fx-background-color: #4f4f4f;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0" minWidth="10.0" prefWidth="480.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="235.0" minWidth="0.0" prefWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="263.0" minHeight="10.0" prefHeight="224.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="161.0" minHeight="0.0" prefHeight="96.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Pane prefHeight="113.0" prefWidth="480.0" style="-fx-background-color: #5d5d5d;" GridPane.rowIndex="1">
               <children>
                  <HBox prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <Button id="button" fx:id="checkButton" mnemonicParsing="false" prefHeight="100.0" prefWidth="160.0" stylesheets="@style/style.css" text="Sprawdź wersję" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Calibri" size="18.0" />
                           </font></Button>
                     </children>
                  </HBox>
                  <HBox layoutX="160.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <Button id="button" fx:id="updateButton" disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="160.0" stylesheets="@style/style.css" text="Aktualizuj" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font name="Calibri" size="18.0" />
                           </font></Button>
                     </children>
                  </HBox>
                  <HBox layoutX="320.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <Button id="button" fx:id="installButton" disable="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="160.0" stylesheets="@style/style.css" text="Zainstaluj paczkę" textAlignment="CENTER" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font></Button>
                     </children>
                  </HBox>
               </children>
            </Pane>
            <TextArea fx:id="textarea" editable="false" prefHeight="200.0" prefWidth="200.0" text="Program aktualizujący paczkę modów do serwera Mizucraft">
               <font>
                  <Font name="Calibri" size="14.0" />
               </font>
            </TextArea>
         </children>
      </GridPane>
   </children>
</Pane>
